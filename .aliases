# For startx select dwm or pantheon
alias sdwm='startx /home/shariyl/.xinitrc'
alias spantheon='startx /home/shariyl/.xinitrc_pantheon'

# utils
alias icat='kitty +kitten icat'
alias refreshenv='. ~/.bash_profile'

# dwm related
alias rdwm2='doas emerge =x11-wm/dwm-6.2 --ask n'
alias cddwm='cd /etc/portage/savedconfig/x11-wm/'
alias lndwm='doas ln /etc/portage/savedconfig/x11-wm/dwm-6.2 /home/shariyl/.config/dwm/dwm-6.2.h'
